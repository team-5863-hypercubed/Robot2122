package frc.robot.commands;

import com.ctre.phoenix.motorcontrol.ControlMode;

import edu.wpi.first.wpilibj2.command.CommandBase;
import frc.robot.subsystems.DriveTrain;
import frc.robot.subsystems.Encoders;
import edu.wpi.first.wpilibj.Joystick;
import edu.wpi.first.wpilibj.XboxController;

public class EncoderTest extends CommandBase {
  private final Encoders m_encoders;

  private double encoderpower;

  /** Creates a new EncoderTest. */
  public EncoderTest(Encoders m_encoders) {
    // Use addRequirements() here to declare subsystem dependencies.
  
    this.m_encoders = m_encoders;
    addRequirements(m_encoders);

    
  }

  @Override
  public void execute() {

    m_encoders.encoderDrive(encoderpower);
  }
  

  // Called once the command ends or is interrupted.
  @Override
  public void end(boolean interrupted) {

  }

  // Returns true when the command should end.
  @Override
  public boolean isFinished() {
    return false;
  }
}
